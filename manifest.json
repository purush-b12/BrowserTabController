{
    "name": "Tabs Assistant",
    "version": "1.0.0",
    "manifest_version": 3,
    "description": "Tabs Assistant",
    "minimum_chrome_version": "100.0",
    "icons":
    {
        "128": "logo.png"
    },
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "popup.html"
    },
    "permissions": ["tabs", "storage", "bookmarks","alarms"]

}
